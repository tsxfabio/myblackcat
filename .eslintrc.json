{
  "extends": ["@rocketseat/eslint-config/node", "plugin:prettier/recommended"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "error",
    "@typescript-eslint/no-explicit-any": "error",
    "camelcase": "off",
    "no-useless-constructor": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
